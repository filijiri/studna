            <string>/// Movement
var new_x;
var update_x = false;

if (key_left_pressed &amp;&amp; !key_right_pressed) {
    new_x = x - move_speed;
    update_x = true;    
}

if (key_right_pressed &amp;&amp; !key_left_pressed) {
    new_x = x + move_speed;
    update_x = true;
}

if (update_x) {
    var limit_left = 16 + sprite_xoffset;
    var limit_right = room_width - 16 - (sprite_width - sprite_xoffset);
    new_x = max(new_x, limit_left);
    new_x = min(new_x, limit_right);
    if (place_free(new_x, y)) {
        x = new_x;
    }
}

