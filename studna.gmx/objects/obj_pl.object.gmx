            <string>/// Handle key
var key_left_pressed = keyboard_check(key_left);
var key_right_pressed = keyboard_check(key_right);

var new_x;
var update_x = false;

if (key_left_pressed &amp;&amp; !key_right_pressed) {
    new_x = x - move_speed;
    update_x = true;    
}

if (key_right_pressed &amp;&amp; !key_left_pressed) {
    new_x = x + move_speed;
    update_x = true;
}

if (update_x) {
    if (place_free(new_x, y)) {
        x = new_x;
    }
}

